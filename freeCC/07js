const myArray = [];
let i = 5;

while (i >= 0) {
    myArray.push(i);
    i--;
}
console.log(myArray)

const myArray = [];

for (let i = 1; i < 6; i++) {
  myArray.push(i);
}



const myArray = [];
for (let i = 1; i < 10; i += 2) {
  myArray.push(i);
}

console.log(myArray)

// Setup
const myArr = [2, 3, 4, 5, 6];

// Only change code below this line

function total () {
   let total = 0;
    for (i= 0; i < myArr.length; i++) {
        total += myArr[i];
     }
    return total;
}
