class Comment {
  constructor(text) {
    this.text = text;
    this.votesQtu = 0;
  }
  upvote() {
    this.votesQtu += 1;
  }
}

const firstComment = new Comment("First comment");

firstComment instanceof Comment;
firstComment.upvote();
console.log(firstComment.votesQtu);
console.log(firstComment);
